<div id="scrumbler_sprint">
    <% if sprint %>
        <div class="contextual">
            <%= link_to "Настройки спринта", project_scrumbler_sprint_settings_url(@project, sprint) %>
        </div>
        <h3><%= sprint.name %></h3>
        <%= draw_scrumbler_dashboard(sprint) %>
    <% else %>
        <p id="errorExplanation"><%=l :error_sprint_not_found_in_project %></p>
    <% end %>
</div>

