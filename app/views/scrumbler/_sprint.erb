<div id="scrumbler_sprint">
    <% if sprint %>
    <div class="contextual">
        <%= link_to "Настройки спринта", settings_project_scrumbler_sprint_url(@project, sprint) %>
    </div>
    <h3><%= sprint.name %></h3>
    <% else %>
        <p id="errorExplanation"><%=l :error_sprint_not_found_in_project %></p>
    <% end %>
</div>

